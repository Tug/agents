package epfl.lia.logist.logging;


/**
 * 
 * @author salves
 *
 */
public enum LogSeverityEnum {
	
	LSV_DEBUG,			/* this level is targetted at debug info */
	LSV_INFO,			/* this level is targetted at general info */
	LSV_WARNING,		/* this level is targetted at warnings */
	LSV_ERROR,			/* this level is targetted at errors */
	LSV_FATAL;			/* this level is targetted at fatal errors */
	
	/**
	 * Returns a string representation of the enumeration
	 * constant.
	 */
	public String toString() {
		switch( this ) {
			case LSV_DEBUG: return "DEBUG";
			case LSV_INFO: return "INFO";
			case LSV_WARNING: return "WARNING";
			case LSV_ERROR: return "ERROR";
			case LSV_FATAL: return "FATAL";
		}
		return "UNKNOWN";
	}
}
