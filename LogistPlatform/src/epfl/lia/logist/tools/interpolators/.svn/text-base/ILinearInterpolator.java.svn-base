package epfl.lia.logist.tools.interpolators;

/**
 * A very primitive interpolator
 */
public interface ILinearInterpolator<E> {
	
	/**
	 * Interpolates from a value to another
	 */
	public E interpolate( double t );
	
	
	/**
	 * Sets the value from which we interpolate
	 */
	public void setFrom( E value );
	
	
	/**
	 * Sets the value to which we interpolate
	 */
	public void setTo( E value );
	
	
	/**
	 * Gets the value from which we interpolate
	 */
	public E getFrom();
	
	
	/**
	 * Gets the value to which we interpolate
	 */
	public E getTo();
}
