package epfl.lia.logist.core.listeners;


import epfl.lia.logist.core.topology.City;

public interface TaskChangeListener {

	/**
	 * Invoked when a city is added
	 * @param city
	 */
	public void onTaskAddition( City city );
	
	/**
	 * Invoked when a city is deleted
	 * @param city
	 */
	public void onTaskDeletion( City city );
	
	/**
	 * Invoked when a city changes one of its values
	 * @param city
	 */
	public void onTaskChange( City city );
}
